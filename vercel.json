{
    "version": 2,
    "builds": [
        {
            "src": "astrology_server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "astrology_server.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.10"
    },
    "functions": {
        "astrology_server.py": {
            "maxDuration": 30
        }
    }
}